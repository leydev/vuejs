{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "plugin:vue/recommended",
        "plugin:vue/strongly-recommended",
        "plugin:vue/essential",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module",
        "project": [
            "./tsconfig.json"
        ],
        "tsconfigRootDir": "./"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-extraneous-dependencies": "off"
    }
}